{
   "background": {
      "scripts": [ "js/libs/jquery/jquery_1.9.1.min.js", "js/background/params.js", "js/background/background.js" ]
   },
   "browser_action": {
      "default_icon": "images/krake_icon_disabled_24.png",
      "default_title": "Release the Kraken!"
   },

   "content_scripts": [ {
      "matches"   : ["<all_urls>"],
      "css": [ "css/page.css" ], 
      "js": [ "js/libs/jquery/jquery_1.9.1.min.js", 
              "js/libs/jquery/jquery_ui_1.10.3.js", 
              "js/content.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "web_accessible_resources": [
    "images/bin.png",
    "images/link.png",
    "html/panel.html"
  ],
   "description": "Extract data with Krake",
   "icons": {
      "128": "images/krake_icon_128.png",
      "16": "images/krake_icon_16.png",
      "24": "images/krake_icon_24.png",
      "32": "images/krake_icon_32.png",
      "48": "images/krake_icon_48.png"
   },
   "manifest_version": 2,
   "name": "Krake.IO",
   "permissions": [ 
                    "contextMenus", 
                    "tabs", 
                    "http://*/*", 
                    "https://*/*"],
   "version": "1.0"
}
